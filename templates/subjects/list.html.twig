{% extends "body.html.twig" %}

{% block suffixTitle %}Subjects list
{% endblock %}

{% block mainContent %}

	<nav class="row mb-3">
		<div class="col-8">
			<twig:Utils:Filters/>
		</div>
		<div class="col-4">
			<twig:Utils:Pagination totalCount="{{ count }}" context="{{ context }}"/>
		</div>
	</nav>

	<ul class="list-group">
		{% for entity in items %}
			<twig:Subject:SubjectRow subject="{{ entity }}"/>
		{% endfor %}
	</ul>

{% endblock %}
